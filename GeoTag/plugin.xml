<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="GeoTag"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="appName"
               value="GeoTag">
         </property>
         <property
               name="windowImages"
               value="icons/geotag_16.png,icons/geotag_32.png,icons/geotag_48.png,icons/geotag_64.png,icons/geotag_128.png,icons/geotag_256.png">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://GeoTag/joachimeichborn.geotag.LifeCycleManager">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
